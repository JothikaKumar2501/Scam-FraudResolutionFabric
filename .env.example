# AWS Bedrock Configuration
AWS_REGION=us-east-1
AWS_CLAUDE_INFERENCE_PROFILE_ARN=arn:aws:bedrock:REGION:ACCOUNT_ID:inference-profile/your-inference-profile
AWS_CLAUDE_MODEL_ID=anthropic.claude-sonnet-4-20250514-v1:0
AWS_TITAN_MODEL_ID=amazon.titan-embed-text-v2:0

# AWS Bedrock AgentCore Memory Configuration
BEDROCK_AGENTCORE_MEMORY_ID=your_agentcore_memory_id_here
AGENTCORE_RUNTIME_ARN=arn:aws:bedrock-agent:REGION:ACCOUNT_ID:agent-runtime/your-runtime-id

# Application Configuration
ENVIRONMENT=production
LOG_LEVEL=INFO

# Qdrant Vector Database Configuration (Optional - for RAG enhancement)
QDRANT_URL=https://your-qdrant-cluster-id.region.gcp.cloud.qdrant.io:6333
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_STUB=0

# Neo4j Database Configuration (Optional - for graph memory)
NEO4J_URI=neo4j+s://your-neo4j-instance.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password_here
NEO4J_DATABASE=neo4j

# Strands Multi-Agent Configuration
USE_INTELLIGENT_DIALOGUE=1
FAST_MODE=0
MAX_CONCURRENT_AGENTS=4
REQUEST_TIMEOUT=30
RETRY_ATTEMPTS=3

# Bedrock Runtime Configuration
BEDROCK_CACHE_TTL=120

# Optional Storage Configuration
S3_BUCKET_NAME=your_s3_bucket_name
VECTOR_DB_ENDPOINT=your_vector_db_endpoint

# Performance Configuration
CACHE_ENABLED=true
ASYNC_PROCESSING=true

# Legacy Configuration (deprecated - remove if not needed)
# MEM0_API_KEY=deprecated_use_agentcore_instead